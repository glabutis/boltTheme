{
  "manifest_version": 2,

  "name": "Bolt theme",
  "description": "A Chrome theme designed to work with Bolt, the new tab extension.",
  "version" : "2.0",



 "theme": {
    "colors" : {
      "frame" : [21, 27, 31],
      "toolbar" : [255, 255, 255],
      "ntp_text" : [21, 27, 31],
      "ntp_link" : [21, 27, 31],
      "ntp_section" : [21, 27, 31],
      "button_background" : [21, 27, 31],
      "tab_text" : [0, 0, 0],
      "frame_inactive" : [21, 27, 31]
    },
    "tints" : {
      "buttons" : [0, 0, 0]
    },
    "properties" : {
      "ntp_background_alignment" : "bottom"
    }
  }
}
